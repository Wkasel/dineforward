{
  "extends": ["tslint:recommended", "tslint-react", "tslint-config-prettier"],
  "rules": {
    "jsx-no-lambda": false,
    "object-literal-sort-keys": false,
    "ordered-imports": false,
    "no-console": false,
    "no-unused-variable": false
  },
  "linterOptions": {
    "exclude": [
      "node_modules/**/*.ts",
      "src/__generated/**/*.tsx",
      "interfaces/**/*.ts",
      "dist/**/*.ts",
      "src/__generated"
    ]
  }
}
